Button>\n                <Modal isOpen={this.state.isModalOpen} toggle={this.toggleModal}>\n                    <ModalHeader toggle={this.toggleModal}>Submit Comment</ModalHeader>\n                    <ModalBody>\n                        <LocalForm onSubmit={(values) => this.handleSubmit(values)}>\n                            <Row className=\"form-group\">\n                                <Label htmlFor=\"author\" md={12}>Your Name</Label>\n                                <Col md={12}>\n                                    <Control.text model=\".author\" id=\"author\" name=\"author\"\n                                        placeholder=\"Your Name\"\n                                        className=\"form-control\"\n                                        validators={{\n                                            required, minLength: minLength(3), maxLength: maxLength(15)\n                                        }}\n                                            />\n                                    <Errors\n                                        className=\"text-danger\"\n                                        model=\".author\"\n                                        show=\"touched\"\n                                        messages={{\n                                            required: 'Required',\n                                            minLength: 'Must be greater than 2 characters',\n                                            maxLength: 'Must be 15 characters or less'\n                                        }}\n                                        />\n                                </Col>\n                            </Row>\n                            <Row className=\"form-group\">\n                                <Label htmlFor=\"rating\" md={12}>Rating</Label>\n                                <Col md={12}>\n                                    <Control.select model=\".rating\" id=\"rating\" name=\"rating\"\n                                        className=\"form-control\">\n                                        <option>1</option>\n                                        <option>2</option>\n                                        <option>3</option>\n                                        <option>4</option>\n                                        <option>5</option>\n                                    </Control.select>\n                                </Col>\n                            </Row>\n                            <Row className=\"form-group\">\n                                <Label htmlFor=\"comment\" md={12}>Comment</Label>\n                                <Col md={12}>\n                                    <Control.textarea model=\".comment\" id=\"comment\" name=\"comment\"\n                                        rows=\"6\"\n                                        className=\"form-control\" />\n                                </Col>\n                            </Row>\n                            <Row className=\"form-group\">\n                                <Col>\n                                    <Button type=\"submit\" color=\"primary\">\n                                    Submit\n                                    </Button>\n                                </Col>\n                            </Row>\n                        </LocalForm>\n                    </ModalBody>\n                </Modal>\n            </div>\n        )\n    }\n}\n\nfunction RenderDish({dish}) {\n    return(\n        <Card>\n            <CardImg top src={dish.image} alt={dish.name} />\n            <CardBody>\n                <CardTitle>{dish.name}</CardTitle>\n                <CardText>{dish.description}</CardText>\n            </CardBody>\n        </Card>\n    );\n}\n\nfunction RenderComments({comments, addComment, dishId}) {\n    if (comments != null) {\n        const comments_list = comments.map((comment) => {\n            return(\n                <div>\n                    <label className=\"mt-3\">{comment.comment}</label>\n                    <label className=\"mt-3\">\n                        -- {comment.author} , {new Intl.DateTimeFormat('en-US', { year: 'numeric', month: 'short', day: '2-digit'}).format(new Date(Date.parse(comment.date)))}\n                    </label>\n                </div>\n            );\n        });\n        return(\n            <div>\n                <h4>Comments</h4>\n                {comments_list}\n                <CommentForm dishId={dishId} addComment={addComment} />\n            </div>\n        );\n    } else  {\n        return(<div></div>);\n    }\n    \n}\n    \nconst  DishDetail = (props) => {\n    if (props.isLoading) {\n        return(\n            <div className=\"container\">\n                <div className=\"row\">            \n                    <Loading />\n                </div>\n            </div>\n        );\n    }\n    else if (props.errMess) {\n        return(\n            <div className=\"container\">\n                <div className=\"row\">            \n                    <h4>{props.errMess}</h4>\n                </div>\n            </div>\n        );\n    }\n    else if (props.dish != null) {\n        return (\n            <div className=\"container\">\n            <div className=\"row\">\n                <Breadcrumb>\n                    <BreadcrumbItem><Link to=\"/menu\">Menu</Link></BreadcrumbItem>\n                    <BreadcrumbItem active>{props.dish.name}</BreadcrumbItem>\n                </Breadcrumb>\n                <div className=\"col-12\">\n                    <h3>{props.dish.name}</h3>\n                    <hr />\n                </div>                \n            </div>\n            <div className=\"row\">\n                <div className=\"col-12 col-md-5 m-1\">\n                    <RenderDish dish={props.dish} />\n                </div>\n                <div className=\"col-12 col-md-5 m-1\">\n                <RenderComments comments={props.comments}\n                                addComment={props.addComment}\n                                dishId={props.dish.id}/>\n                </div>\n            </div>\n            </div>\n        );\n    } else {\n        return(<div></div>);\n    }\n}\n\n\nexport default DishDetail;",["100","101"],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/reportWebVitals.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/index.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/components/MainComponent.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/shared/dishes.js",[],["102","103"],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/components/FooterComponent.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/components/HeaderComponent.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/components/ContactComponent.js",["104","105","106","107","108","109","110"],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/components/HomeComponent.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/shared/comments.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/components/AboutComponent.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/shared/leaders.js",[],["111","112"],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/shared/promotions.js",[],["113","114"],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/redux/configureStore.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/redux/promotions.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/redux/leaders.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/redux/comments.js",[],"/home/jorge/Documentos/Estudios/Tecnologias/React/react-confusion/src/redux/dishes.js",[],"/home/jorge/Docu